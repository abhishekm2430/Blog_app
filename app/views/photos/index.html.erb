<div class="page-header"><h1>Upload Photo</h1></div>

<div>
  <% @photos.each do |photo| %>
    <div class="comp">
      <!-- Renders image -->
      <%= link_to image_tag(photo.image.url), photo.image.url, target: '_blank' %>
    </div>
    <div>
      <h4><%=link_to photo.title, photo_path(photo), method: :delete, data: {confirm: 'Are you sure'} %></h4>
    </div>
  <% end %>
</div>